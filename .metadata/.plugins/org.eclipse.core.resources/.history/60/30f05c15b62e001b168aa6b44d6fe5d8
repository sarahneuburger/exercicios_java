package LacosDeRepeticao;

public class Atividade3 {
	
	public static void main(String[] args) {
		
		// Laço While
		
		int numero1 = 101;
		
		int numero2 = 200;
		
		while ((numero1 + 1) %  2 == 0 && numero1 <= numero2)  {
			
			System.out.println(numero1);
			numero1++;
			
		}
		
		
		
	}	

}
