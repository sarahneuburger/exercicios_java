package LacosDeRepeticao;

public class Atividade7 {

	public static void main(String[] args) {
		
		// Laço Do While
		
		
		int numero1 = 101;
		
		int numero2 = 200;
		
		do {
			
			if (numero1 % 2 == 0) {
			
			System.out.println(numero1);

			}
			
			numero1++;	
			
		} while (numero1 <= numero2);
		
			
	}	
}
