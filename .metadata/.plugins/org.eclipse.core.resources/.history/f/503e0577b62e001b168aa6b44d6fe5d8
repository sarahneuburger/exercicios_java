package LacosDeRepeticao;

public class Atividade3 {
	
	public static void main(String[] args) {
		
		// Laço While
		
		int numero1 = 101;
		
		int numero2 = 200;
		
		do {
			
			System.out.println(numero1);
			numero1++;	
			
			if (numero1 % 2 == 0);
		
		} while (numero1 <= numero2);
		
	}	

}
